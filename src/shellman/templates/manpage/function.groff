.SH "FUNCTIONS"
{% for function in section -%}
.IP "{{ function.prototype|groff_auto_escape|groff_strong }}" 4
{{ function.brief }}

{% if function.description %}
{{ function.description }}
{% endif %}

{% if function.arguments %}
{{ "Arguments"|groff_strong }}
{% for argument in function.arguments %}
{{ " " * context.indent }}{{ argument|first_word|groff_strong }} - {{ argument|body }}
{% endfor %}
{% endif %}

{% if function.return_codes %}
{{ "Return codes"|groff_strong }}
{% for return_code in function.return_codes %}
{{ " " * context.indent }}{{ return_code|first_word|groff_strong }} - {{ return_code|body }}
{% endfor %}
{% endif %}

{% if function.preconditions %}
{{ "Pre-conditions"|groff_strong }}
{% for precondition in function.preconditions %}
{{ " " * context.indent }}{{ precondition }}
{% endfor %}
{% endif %}

{% if function.seealso %}
{{ "See also"|groff_strong }}
{% for seealso in function.seealso %}
{{ " " * context.indent }}{{ seealso }}
{% endfor %}
{% endif %}

{% if function.stderr %}
{% endif %}

{% if function.stdin %}
{% endif %}

{% if function.stdout %}
{% endif %}

{% endfor %}
